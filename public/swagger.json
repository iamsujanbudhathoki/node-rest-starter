		"schemas": {
			"MediaType": {
				"enum": [
					"PROFILE_IMAGE",
					"PRODUCT_IMAGE",
					"CAROUSEL_IMAGE",
					"STORE_LOGO",
					"BLOG_THUMBNAIL",
					"CAREER_CV"
				],
				"type": "string"
			},
			"Media": {
				"properties": {
					"id": {
						"type": "string"
					},
					"createdAt": {
						"type": "string",
						"format": "date-time"
					},
					"deletedAt": {
						"type": "string",
						"format": "date-time"
					},
					"mimeType": {
						"type": "string"
					},
					"name": {
						"type": "string"
					},
					"fileSize": {
						"type": "string"
					},
					"mediaType": {
						"$ref": "#/components/schemas/MediaType"
					},
					"path": {
						"type": "string"
					}
				},
				"required": [
					"id",
					"createdAt",
					"deletedAt",
					"mimeType",
					"name",
					"fileSize",
					"mediaType",
					"path"
				],
				"type": "object",
				"additionalProperties": false
			},
			"AdminAuthSchema": {
				"properties": {
					"email": {
						"type": "string"
					},
					"password": {
						"type": "string"
					}
				},
				"required": [
					"email",
					"password"
				],
				"type": "object",
				"additionalProperties": false
			}
		},
		"securitySchemes": {}
